<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Subject</title>
    <link type="text/css" rel="stylesheet" href="/style.css">
  </head>

  <body>
    <h1>Subject Data</h1>
    <div class="title">
      <table border="1">
        <tr>
          <th> ID </th>
          <th> Subject Name</th>
        </tr>
        <%subject_data.forEach(s => { %>
          <tr>
            <td><%= s.id %> </td>
            <td><%= s.subject_name %></td>
          </tr>
        <% }) %>
      </table>
      <br>
      <br>
      <a href="/">home</a>
    </div>
  </body>
</html>
