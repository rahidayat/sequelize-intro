<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Add Subject to Student</title>
    <link type="text/css" rel="stylesheet" href="/style.css">
  </head>

  <body>

    <h1><%= enrolled_data[0].Subject.subject_name %></h1>


    <a href="/subjects">All Subjects List</a>

    <div class="title">
      <table border="1">
        <tr>
          <th>Name</th>
          <th>Score</th>
        </tr>
        <%enrolled_data.forEach(e => { %>
        <tr>
            <td><%=e.Student.first_name%> <%=e.Student.last_name%></td>
            <td><% if (e.score==null) { %>
              <a href="/subjects/<%=e.id %>/givescore">Give Score</a>
          <%  } else{%><%=e.score%><%} %>
              </td>
        </tr>
        <% }) %>
      </table>

      <br>
      <br>
      <a href="/">home</a>
    </div>
  </body>
</html>
